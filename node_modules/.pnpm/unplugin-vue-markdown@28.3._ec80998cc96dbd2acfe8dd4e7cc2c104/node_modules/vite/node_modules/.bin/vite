#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Administrator/Documents/GitHub/blogexample.github.io/node_modules/.pnpm/vite@6.2.3_@types+node@22.1_93745fe48431614d00d9c88fa9857894/node_modules/vite/bin/node_modules:/mnt/c/Users/Administrator/Documents/GitHub/blogexample.github.io/node_modules/.pnpm/vite@6.2.3_@types+node@22.1_93745fe48431614d00d9c88fa9857894/node_modules/vite/node_modules:/mnt/c/Users/Administrator/Documents/GitHub/blogexample.github.io/node_modules/.pnpm/vite@6.2.3_@types+node@22.1_93745fe48431614d00d9c88fa9857894/node_modules:/mnt/c/Users/Administrator/Documents/GitHub/blogexample.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Administrator/Documents/GitHub/blogexample.github.io/node_modules/.pnpm/vite@6.2.3_@types+node@22.1_93745fe48431614d00d9c88fa9857894/node_modules/vite/bin/node_modules:/mnt/c/Users/Administrator/Documents/GitHub/blogexample.github.io/node_modules/.pnpm/vite@6.2.3_@types+node@22.1_93745fe48431614d00d9c88fa9857894/node_modules/vite/node_modules:/mnt/c/Users/Administrator/Documents/GitHub/blogexample.github.io/node_modules/.pnpm/vite@6.2.3_@types+node@22.1_93745fe48431614d00d9c88fa9857894/node_modules:/mnt/c/Users/Administrator/Documents/GitHub/blogexample.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
